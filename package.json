{
  "name": "mxcd.template.ts",
  "version": "1.1.3",
  "private": false,
  "main": "index.js",
  "keywords": ["template", "ts", "js"],
  "scripts": {
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "delete": "node scripts/delete.js",
    "build": "npm run delete && npm run tojs && npm run copy",
    "copy": "node scripts/copy.js",
    "tojs": "tsc --sourceMap --esModuleInterop --declaration --outDir dist src/index.ts",
    "publish:dist": "npm publish ./dist"
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-junit-reporter",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "cobertura",
      "html"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "11.12.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "jest-junit-reporter": "^1.1.0"
  }
}
